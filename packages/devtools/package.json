{
  "name": "@formwerk/devtools",
  "version": "0.14.4",
  "description": "Formwerk's Vue devtools plugin",
  "keywords": [
    "Vue",
    "VueJS",
    "form",
    "inputs",
    "validation",
    "validator"
  ],
  "homepage": "https://formwerk.dev/",
  "license": "MIT",
  "author": "Abdelrahman Awad <logaretm1@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/formwerkjs/formwerk.git",
    "directory": "packages/devtools"
  },
  "funding": "https://github.com/sponsors/logaretm",
  "files": [
    "dist/*.cjs",
    "dist/*.d.ts",
    "dist/*.js",
    "dist/*.mjs"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.mjs",
  "module": "dist/devtools.mjs",
