      "require": "./dist/devtools.cjs"
    },
    "./dist/index.d.ts": "./dist/devtools.d.ts"
  },
  "dependencies": {
    "@vue/devtools-api": "^8.0.5",
    "@vue/devtools-kit": "^8.0.5"
  }
